{
  "name": "utools-logseq-saver",
  "version": "0.0.1",
  "pluginName": "快速保存至 Logseq",
  "description": "快速保存文本、图片、视频等到 Logseq",
  "logo": "logo-iconfont.png",
  "preload": "preload.js",
  "features": [
    {
      "code": "save_to_logseq",
      "explain": "保存至 Logseq",
      "cmds": [
        "save to logseq",
        {
          "type": "over",
          "label": "保存至 Logseq"
        },
        {
          "type": "img",
          "label": "保存至 Logseq"
        },
        {
          "type": "files",
          "label": "保存至 Logseq",
          "fileRype": "file"
        }
      ]
    },
    {
      "code": "set_logseq_server_host",
      "explain": "设置 Logseq Server Host",
      "cmds": ["set logseq server host"]
    },
    {
      "code": "set_logseq_server_port",
      "explain": "设置 Logseq Server Port",
      "cmds": ["set logseq server port"]
    },
    {
      "code": "set_logseq_server_token",
      "explain": "设置 Logseq Server Token",
      "cmds": ["set logseq server token"]
    },
    {
      "code": "get_logseq_server_config",
      "explain": "获取 Logseq Server Config",
      "cmds": ["get logseq server config"]
    }
  ]
}
