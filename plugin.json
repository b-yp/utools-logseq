{
  "name": "utools-logseq-saver",
  "version": "0.0.1",
  "pluginName": "快速保存至 Logseq",
  "description": "快速保存文本、图片、视频等到 Logseq",
  "logo": "logo.png",
  "preload": "preload.js",
  "features": [
    {
      "code": "save_to_logseq",
      "explain": "保存至Logseq",
      "cmds": [
        "save to logseq",
        {
          "type": "over",
          "label": "保存至Logseq"
        },
        {
          "type": "img",
          "label": "保存至Logseq"
        },
        {
          "type": "files",
          "label": "保存至Logseq",
          "fileRype": "file"
        }
      ]
    },
    {
      "code": "configs",
      "explain": "设置",
      "cmds": ["Configs"]
    },
    {
      "code": "search",
      "explain": "搜索",
      "cmds": [
        "search",
        {
          "type": "over",
          "label": "搜索"
        }
      ]
    },
    {
      "code": "graph",
      "explain": "切换库",
      "cmds": ["graph"]
    }
  ]
}
